@model List<SellLog>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>Registro delle vendite</h1>
        </div>
    </div>
  <div class="row">
    <div class="col-md-12">
        @if(Model.Count == 0){
            <h2>Non ci sono ancora log presenti</h2>
        }
        else{
      <table class="table">
        <thead>
          <tr>
            <th>ID</th>
            <th>Data</th>
            <th>Cliente</th>
            <th>Quantità</th>
            <th>Prezzo</th>
          </tr>
        </thead>
        <tbody>
        @foreach (SellLog log in Model)
        {
          <tr>
            <th>@log.Id</th>
            <th>@log.Date</th>
            <th>@log.Customer</th>
            <th>@log.Quantity</th>
            <th>@log.Price</th>
          </tr>
        }
        </tbody>
      </table>
        }
    </div>
  </div>
</div>